{
  "name": "fbabuse",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "js": ["thirdparty/jquery-3.4.1.min.js","content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "FBabuse"
  }
}